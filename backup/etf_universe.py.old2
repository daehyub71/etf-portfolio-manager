    def load_etf_list(self, file_path: str) -> bool:
        """CSVì—ì„œ ETF ëª©ë¡ ê°€ì ¸ì˜¤ê¸°"""
        try:
            df = pd.read_csv(file_path, encoding='utf-8-sig')
            
            for _, row in df.iterrows():
                code = row['code']
                etf_info = row.drop('code').to_dict()
                
                # NaN ê°’ì„ Noneìœ¼ë¡œ ë³€í™˜
