    def save_to_database(self, db_path: str = None) -> bool:
        """ETF ì •ë³´ë¥¼ ë°ì´í„°ë² ì´ìŠ¤ì— ì €ì¥"""
        target_db = db_path or self.db_path
        if not target_db:
            logger.warning("ë°ì´í„°ë² ì´ìŠ¤ ê²½ë¡œê°€ ì§€ì •ë˜ì§€ ì•Šì•˜ìŠµë‹ˆë‹¤")
            return False
        
        try:
            conn = sqlite3.connect(target_db)
            
            # í…Œì´ë¸”ì˜ ì‹¤ì œ ì»¬ëŸ¼ êµ¬ì¡° í™•ì¸
            cursor = conn.execute("PRAGMA table_info(etf_info)")
            table_info = cursor.fetchall()
            
            if not table_info:
                # í…Œì´ë¸”ì´ ì—†ìœ¼ë©´ ìƒì„± (ê¸°ë³¸ êµ¬ì¡°ë¡œ)
                conn.execute('''
                    CREATE TABLE IF NOT EXISTS etf_info (
                        code TEXT PRIMARY KEY,
                        name TEXT NOT NULL,
                        category TEXT,
                        subcategory TEXT,
                        asset_class TEXT,
                        region TEXT,
                        currency TEXT DEFAULT 'KRW',
                        expense_ratio REAL,
                        aum REAL DEFAULT 0,
                        market_price REAL DEFAULT 0,
                        benchmark TEXT,
                        fund_manager TEXT,
                        description TEXT,
                        last_updated TEXT
                    )
                ''')
                logger.info("etf_info í…Œì´ë¸” ìƒì„± ì™„ë£Œ")
            
            # ì‚¬ìš© ê°€ëŠ¥í•œ ì»¬ëŸ¼ ëª©ë¡
            cursor = conn.execute("PRAGMA table_info(etf_info)")
            available_columns = [column[1] for column in cursor.fetchall()]
            
            # ì»¬ëŸ¼ëª… ë§¤í•‘
            column_mapping = {
                'tracking_index': 'benchmark',  # tracking_indexë¥¼ benchmarkë¡œ ë§¤í•‘
                'fund_company': 'fund_manager'   # fund_companyë¥¼ fund_managerë¡œ ë§¤í•‘
            }
            
            # ETF ë°ì´í„° ì €ì¥
            saved_count = 0
            for code, etf_info in self.etf_data.items():
                # ì €ì¥í•  ë°ì´í„° ì¤€ë¹„
                save_data = {'code': code}
                
                # ê¸°ë³¸ í•„ë“œë“¤ ë§¤í•‘
                field_mapping = {
                    'name': etf_info.get('name'),
                    'category': etf_info.get('category'),
                    'subcategory': etf_info.get('subcategory'),
                    'asset_class': etf_info.get('asset_class'),
                    'region': etf_info.get('region'),
                    'currency': etf_info.get('currency', 'KRW'),
                    'expense_ratio': etf_info.get('expense_ratio'),
                    'aum': etf_info.get('aum'),
                    'market_price': etf_info.get('market_price'),
                    'description': etf_info.get('description'),
                    'last_updated': datetime.now().isoformat()
                }
                
                # ì»¬ëŸ¼ëª… ë§¤í•‘ ì ìš©
                for source_field, target_field in column_mapping.items():
                    if source_field in etf_info and target_field in available_columns:
                        field_mapping[target_field] = etf_info[source_field]
                
                # ì‚¬ìš© ê°€ëŠ¥í•œ ì»¬ëŸ¼ë§Œ ì €ì¥
                final_data = {}
                for field, value in field_mapping.items():
                    if field in available_columns:
                        final_data[field] = value
                
                # codeëŠ” í•­ìƒ í¬í•¨
                final_data['code'] = code
                
                # ë™ì  INSERT/REPLACE ì¿¼ë¦¬ ìƒì„±
                columns = list(final_data.keys())
                placeholders = ['?' for _ in columns]
                values = [final_data[col] for col in columns]
                
                query = f'''
                    INSERT OR REPLACE INTO etf_info 
                    ({', '.join(columns)})
                    VALUES ({', '.join(placeholders)})
                '''
                
                conn.execute(query, values)
                saved_count += 1
            
            conn.commit()
            conn.close()
            
            logger.info(f"ETF ì •ë³´ {saved_count}ê°œë¥¼ ë°ì´í„°ë² ì´ìŠ¤ì— ì €ì¥ ì™„ë£Œ")
            return True
            
        except Exception as e:
            logger.error(f"ë°ì´í„°ë² ì´ìŠ¤ ì €ì¥ ì‹¤íŒ¨: {e}")
            return False


